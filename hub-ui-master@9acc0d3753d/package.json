{
  "name": "piveau-hub-ui",
  "description": "A Single-Page-Application base to realize Open Data Frontends.",
  "author": "Dennis Ritter (dennis.ritter@fokus.fraunhofer.de)",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "postinstall": "patch-package",
    "dev": "vue-cli-service serve",
    "serve": "vue-cli-service serve",
    "build": "cross-env NODE_ENV=production BUILD_MODE=build vue-cli-service build --modern",
    "build:test": "cross-env NODE_ENV=production BUILD_MODE=test vue-cli-service build --modern",
    "test:e2e": "vue-cli-service test:e2e",
    "lint": "vue-cli-service lint",
    "cy": "vue-cli-service test:e2e",
    "cy:unit": "cypress open-ct",
    "cy:unit:headless": "cypress run-ct",
    "cy:run:chrome": "vue-cli-service test:e2e --browser chrome",
    "cy:run:firefox": "vue-cli-service test:e2e --browser firefox",
    "cy:ci:chrome": "vue-cli-service test:e2e --headless --browser chrome",
    "cy:ci:firefox": "vue-cli-service test:e2e --headless --browser firefox",
    "cy:ci": "npm run cy:ci:firefox",
    "k6": "k6 run k6/tests.js",
    "k6:testUI": "k6 run k6/hub-ui/test-hub-ui-API.js",
    "k6:testRepoLite": "k6 run k6/hub-repo/test-hub-repo-API-lite.js",
    "k6:testRepoIDs": "k6 run -e DATASET_ID=%DATASET_ID% k6/hub-repo/test-hub-repo-API-IDs.js",
    "k6:testRepo": "k6 run k6/hub-repo/test-hub-repo-API.js"
  },
