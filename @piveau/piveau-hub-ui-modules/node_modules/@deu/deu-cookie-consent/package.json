{
  "name": "@deu/deu-cookie-consent",
  "author": "Duy Minh Vo <duy.minh.vo@fokus.fraunhofer.de>",
  "version": "2.5.0",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint",
    "build:lib": "vue-cli-service build --target lib --name deu-cookie-consent src/index.js",
    "semantic-release": "semantic-release"
  },
  "main": "dist/deu-cookie-consent.umd.js",
  "module": "dist/deu-cookie-consent.esm.js",
  "unpkg": "dist/deu-cookie-consent.min.js",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "registry": "https://paca.fokus.fraunhofer.de/repository/npm-hosted/"
  },
  "dependencies": {
    "@piveau/piveau-cookie-consent": "^1.1.3"
  },
  "devDependencies": {
    "@piveau/piveau-universal-piwik": "^2.1.2",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/gitlab": "^9.1.2",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-unit-jest": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/test-utils": "^1.0.3",
    "babel-eslint": "^10.1.0",
    "core-js": "^3.22.4",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "sass": "^1.35.2",
    "sass-loader": "^10.2.0",
    "semantic-release": "^19.0.2",
    "tiny-cookie": "^2.3.2",
    "vue": "^2.6.14",
    "vue-template-compiler": "^2.6.11"
  },
  "license": "Apache-2.0",
  "release": {
    "branches": [
      {
        "name": "master"
      },
      {
        "name": "next",
        "channel": "next"
      },
      {
        "name": "beta",
        "prerelease": true
      },
      "+([0-9])?(.{+([0-9]),x}).x"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm",
      [
        "@semantic-release/gitlab",
        {
          "gitlabUrl": "https://gitlab.fokus.fraunhofer.de"
        }
      ],
      [
        "@semantic-release/git",
        {
          "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ]
    ]
  }
}
